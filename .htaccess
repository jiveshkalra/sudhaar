RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]

ErrorDocument 403 http://artificial-doctor.epizy.com/error_404.php

ErrorDocument 404 http://artificial-doctor.epizy.com/error_404.php

ErrorDocument 500 http://artificial-doctor.epizy.com/error_404.php